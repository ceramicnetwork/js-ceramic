{
  "moduleFileExtensions": ["js", "json", "ts"],
  "testEnvironment": "node",
  "extensionsToTreatAsEsm": [".ts"],
  "testRegex": "(/__tests__/.*(\\.|/)(test|spec))\\.tsx?$",
  "moduleNameMapper": {
    "^(\\.{1,2}/.*)\\.js$": "$1"
  },
  "transform": {
    "node_modules/variables/.+\\.(j|t)sx?$": "ts-jest"
  },
  "transformIgnorePatterns": [
    "node_modules/(?!variables/.*)"
  ]
}
