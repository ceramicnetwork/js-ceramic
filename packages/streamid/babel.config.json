{
	"presets": [
		["@babel/preset-env", {
			"targets": {
				"node": "current"
			},
			"modules": "commonjs",
			"useBuiltIns": false,
			"debug": false
		}],
		["@babel/preset-typescript", {
      "allowNamespaces": true
    }]
	],
	"plugins": [
		["@babel/plugin-transform-modules-commonjs", {
			"allowTopLevelThis": true
		}],
		["@babel/plugin-proposal-decorators", {"legacy": true}]
	]
}

