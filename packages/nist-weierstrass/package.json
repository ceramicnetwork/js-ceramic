{
  "name": "nist-weirstrauss",
  "version": "2.0.4",
  "description": "Utilities for NIST curves for the Ceramic did:key method resolver",
  "keywords": [
    "ceramic",
    "nist",
    "weirstrauss"
  ],
  "author": "Brent Shambaugh <brent.shambaugh@gmail.com>",
  "license": "(Apache-2.0 OR MIT)",
  "directories": {
    "lib": "./lib",
    "src" : "./src"
  },
  "main": "./lib/index.js",
  "source": "src/index.ts",
  "types": "./lib/index.d.ts",
  "type": "module",
  "exports": {
    ".": "./lib/index.js"
  },
  "files": [
    "lib",
    "src"
  ],
  "sideEffects": false,
  "scripts": {
    "test": "NODE_OPTIONS=--experimental-vm-modules npx jest --silent --coverage --forceExit",
    "build": "npx tsc --project tsconfig.json",
    "prepublishOnly": "npm run build",
    "prebuild": "npm run clean",
    "lint": "npx eslint ./src --ext .js,.jsx,.ts,.tsx",
    "clean": "npx rimraf ./lib"
  },
  "dependencies": {
    "multiformats": "^9.5.2",
    "uint8arrays": "^3.0.0",
    "bigint-mod-arith": "^3.1.0"
