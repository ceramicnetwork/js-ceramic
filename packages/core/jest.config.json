{
  "roots": [
    "<rootDir>",
    "node_modules"
  ],
  "modulePaths": [
    "<rootDir>",
    "node_modules"
  ],
  "resolver": "jest-resolver-enhanced",
  "testMatch": ["**/?(*.)+(spec|test).[jt]s?(x)"],
  "extensionsToTreatAsEsm": [".ts"],
  "moduleNameMapper": {
    "^(\\.{1,2}/.*)\\.js$": "$1"
  },
  "transform": {
    "^.+\\.tsx?$": "babel-jest"
  },
  "moduleDirectories": [
    "node_modules",
    "src"
  ]
}
